# Google OAuth credentials
# Get these from the Google Cloud Console: https://console.cloud.google.com/
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret

# Google Maps API Key
# Get this from the Google Cloud Console: https://console.cloud.google.com/
GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# Session secret for Express session
# This should be a random string
SESSION_SECRET=your_random_session_secret

# Port for the server (optional, defaults to 3000)
PORT=3000

# Node environment (development or production)
NODE_ENV=development
